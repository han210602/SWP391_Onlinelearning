<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>LoginController.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterController</servlet-name>
        <servlet-class>LoginController.RegisterController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FotgettPassword</servlet-name>
        <servlet-class>LoginController.FotgotPassword</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>homePageController</servlet-name>
        <servlet-class>AdminController.homePageController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Course</servlet-name>
        <servlet-class>model.Course</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DashboardController</servlet-name>
        <servlet-class>AdminController.DashboardController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CouseManager</servlet-name>
        <servlet-class>AdminController.CouseManager</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CoursesManager</servlet-name>
        <servlet-class>AdminController.CoursesManager</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterController</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FotgettPassword</servlet-name>
        <url-pattern>/foget</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>homePageController</servlet-name>
        <url-pattern>/homeadmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Course</servlet-name>
        <url-pattern>/course</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DashboardController</servlet-name>
        <url-pattern>/dashboard</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CouseManager</servlet-name>
        <url-pattern>/cousemanager</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CoursesManager</servlet-name>
        <url-pattern>/coursesmanager</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
